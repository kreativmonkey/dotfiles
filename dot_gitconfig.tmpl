[user]
	email = {{ .email }}
	name = {{ .name }}
[core]
	editor = nvim
	autocrlf = input
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
[merge]
	conflictstyle = diff3
	tool = meld
[diff]
	colorMoved = default
[init]
	defaultBranch = main
[cola]
	spellcheck = false
